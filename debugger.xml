<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="valueLookupDelay" value="150" />
    </data-views>
    <general />
    <debuggers>
      <debugger id="dotnet_debugger">
        <configuration>
          <option name="remappedSourceLocationsMap">
            <map />
          </option>
        </configuration>
      </debugger>
      <debugger id="kotlin_debugger">
        <configuration>
          <option name="DEBUG_IS_FILTER_FOR_STDLIB_ALREADY_ADDED" value="true" />
        </configuration>
      </debugger>
      <debugger id="oracle">
        <configuration>
          <option name="steppingPauseAtBegin" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>